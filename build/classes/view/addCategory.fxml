<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Text?>

<BorderPane fx:id="addCategoryPane" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.AddCategoryPaneController">
   <top>
      <Label text="Aggiungi Categoria" BorderPane.alignment="CENTER" />
   </top>
   <center>
      <Pane BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="addCategoryBtn" layoutX="321.0" layoutY="188.0" mnemonicParsing="false" onAction="#addCategoryBtnClicked" text="Aggiungi" />
            <TextField fx:id="textField" layoutX="109.0" layoutY="83.0" />
            <Text layoutX="7.0" layoutY="100.0" strokeType="OUTSIDE" text="Nome categoria" />
         </children>
      </Pane>
   </center>
</BorderPane>
